@model IEnumerable<MvcLabWeb.Models.TravelAlert>
@{
    ViewBag.Title = "Index";
}

<h2>國際間旅遊疫情建議等級表</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.sent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.source)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.effective)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.expires)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.senderName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.headline)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.instruction)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.web)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.alert_title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.severity_level)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.alert_disease)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.areaDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.areaDesc_EN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.circle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ISO3166)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.areaDetail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ISO3166_2)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.sent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.source)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.effective)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.expires)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.senderName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.headline)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.instruction)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.web)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.alert_title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.severity_level)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.alert_disease)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.areaDesc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.areaDesc_EN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.circle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ISO3166)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.areaDetail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ISO3166_2)
            </td>
        </tr>
    }
</table>